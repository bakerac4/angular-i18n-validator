{
    "name": "angular-i18n-validator",
    "displayName": "Angular 6+ i18n validation and more",
    "description": "i18n validation for Angular 6 project",
    "author": "Oleksandr Reznichenko",
    "license": "MIT",
    "version": "1.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/rettoua/angular-i18n-validator"
    },
    "publisher": "OleksandrReznichenko",
    "categories": [],
    "icon": "images/icon.png",
    "keywords": [
        "multi-root ready"
    ],
    "engines": {
        "vscode": "^1.25.0"
    },
    "activationEvents": [
        "onLanguage:html",
        "onLanguage:xml"
    ],
    "main": "./client/out/extension",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run compile",
        "compile": "tsc -b",
        "watch": "tsc -b -w",
        "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
        "test": "sh ./scripts/e2e.sh"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.6",
        "@types/node": "^8.10.42",
        "tslint": "^5.13.1",
        "typescript": "^3.3.3333"
    },
    "dependencies": {
        "fs-extra": "^7.0.1"
    }
}
